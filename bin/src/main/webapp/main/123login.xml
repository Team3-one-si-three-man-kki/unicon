<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head meta_templateCategory="01_Single" meta_templateName="1_06 login">
        <w2:type>COMPONENT</w2:type>
        <w2:buildDate/>
        <w2:MSA/>
        <xf:model>
            <w2:dataCollection baseNode="map"/>
            <w2:workflowCollection/>
        </xf:model>
        <w2:layoutInfo/>
        <w2:publicInfo method=""/>
        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {
	
};
]]></script>
    <style><![CDATA[
  .login_wrap {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
  }
  .loginbox {
  width: 50%;
  margin: 0 auto;
}
  .left{
    width:50%; height:100vh; background-color:black;
  }
  .right{
    width:50%; height:100vh; background-color:white; min-width:500px
  }

  @media (max-width: 768px) {
    .left, .right {
      width: 100% !important;
      height: auto !important;
    }
      .loginbox {
    width: 70%;
  }
  }
]]></style>

    </head>
    <body class="" ev:onpageload="scwin.onpageload">
        <xf:group class="wrap_containter" id="" style=''>
        <xf:group id="left" class="left" style=""><xf:group tagname="h1" id="" class="login_logo">
                <w2:anchor outerDiv="false" style="" id="" title="Inswave">
                    <xf:label><![CDATA[]]></xf:label>
                </w2:anchor>
            </xf:group></xf:group><xf:group id="right" class="right" style="display: flex;flex-direction: column;justify-content: center;   /* 세로 정렬 */align-items: center;       /* 가로 정렬 */"><xf:group id="" class="loginbox" style="">
                <xf:group id="" class="login_header">
                    <w2:textbox style="" id="" label="WELCOME"></w2:textbox>
                    <w2:textbox style="" id="" label="SIGN IN TO INSWAVE" class="login_info"></w2:textbox>
                </xf:group>
                <xf:group style="" id="" class="login_contents">
                    <xf:group tagname="ul" id="">
                        <xf:group tagname="li" id="">
                            <w2:textbox tagname="label" for="user_id" style="" id="" label="ID" class="tit"></w2:textbox>
                            <xf:input adjustMaxLength="false" style="" id="" placeholder="아이디를 입력해주세요."></xf:input>
                        </xf:group>
                        <xf:group tagname="li" id="">
                            <w2:textbox tagname="label" for="user_pw" style="" id="" label="Password" class="tit"></w2:textbox>
                            <xf:group id="" class="login_form">
                                <xf:input adjustMaxLength="false" style="" id="" placeholder="비밀번호를 입력해주세요." type="password"></xf:input>
                                <w2:anchor outerDiv="false" style="" id="" class="btn_login_password">
                                    <xf:label><![CDATA[비밀번호 보기]]></xf:label>
                                </w2:anchor>
                            </xf:group>
                            <xf:group id="" class="grp_password">
                                <xf:select ref="" appearance="full" style="" id="" rows="" selectedindex="-1" cols="">
                                    <xf:choices>
                                    <xf:item>
                                    <xf:label><![CDATA[Remember me]]></xf:label>
                                    <xf:value><![CDATA[]]></xf:value>
                                    </xf:item>
                                    </xf:choices>
                                </xf:select>
                                <w2:anchor outerDiv="false" style="" id="" class="link_password">
                                    <xf:label><![CDATA[Forgot password?]]></xf:label>
                                </w2:anchor>
                            </xf:group>
                        </xf:group>
                    </xf:group>
                    <xf:group style="" id="" class="login_btn">
                        <xf:trigger style="" id="" type="button" class="btn_login">
                            <xf:label><![CDATA[Login]]></xf:label>
                        </xf:trigger>
                    </xf:group>
                    <xf:group style="" id="" class="login_account">
                        <w2:textbox style="" id="" label="New user?" class=""></w2:textbox>
                        <w2:anchor outerDiv="false" style="" id="" class="link_account">
                            <xf:label><![CDATA[Create an account]]></xf:label>
                        </w2:anchor>
                    </xf:group>
                </xf:group>
            </xf:group></xf:group></xf:group>
    </body>
</html>
